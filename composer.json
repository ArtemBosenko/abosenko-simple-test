{
  "name": "abosenko/simple-test",
  "description": "Simple test task",
  "type": "test-project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-pdo": "*"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    },
    "sort-packages": true
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*"
  }
}
